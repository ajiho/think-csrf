    },
    "files": [
      "src/helper.php"
    ]
  },
  "authors": [
    {
      "name": "ajiho",
      "email": "lujiahao@88.com",
      "homepage": "https://gitee.com/ajiho",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "topthink/framework": "^6.0.0"
  },
  "extra": {
    "think": {
      "config": {
        "csrf": "src/config.php"
      }
    }
  }
}