{
  "name": "ajiho/think-csrf",
  "description": "Thinkphp6 solves CSRF attacks",
  "keywords": [
    "thinkphp6",
    "csrf"
  ],
  "homepage": "https://gitee.com/ajiho/think-csrf",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
